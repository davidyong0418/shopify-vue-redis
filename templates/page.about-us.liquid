  /* Dynamic class styles */
  .current {
    color: rgb(234, 204, 206);
    font-weight: bold;
    opacity: 1;
    animation-name: fadeInOpacity;
    animation-iteration-count: 1;
    animation-timing-function: ease-in;
    animation-duration: 400ms;
    position: relative;
  }

  .current:after {
    width: 0;
    height: 0;
    border-left: 15px solid transparent;
    border-right: 15px solid transparent;
    border-top: 15px solid rgb(234, 204, 206);
    content: " ";
    position: absolute;
    top: 100%;
  }

  .current.button__about-us:after {
    left: 26.5%;
  }

  .current.button__our-story:after {
    left: 31.7%;
  }

  .current.button__our-mission:after {
    left: 34%;
